     </cache>
     
     <cache name="languages"
           maxEntriesLocalHeap="1000"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>  
     </cache>
     
     <cache name="prgmLanguajes"
           maxEntriesLocalHeap="1000"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>  
     </cache>
     
     <!-- not in operation -->
     <cache name="ar.project.ent.entities.area.Country"
           maxEntriesLocalHeap="1000"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LRU">
